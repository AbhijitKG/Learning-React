{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${file}",
                "outFiles": [
                    "${workspaceRoot}/out/**/*.js"
                ]
            },
            {
                "name": "Debug iOS",
                "program": "${workspaceRoot}/.vscode/launchReactNative.js",
                "type": "reactnative",
                "request": "launch",
                "platform": "ios",
                "sourceMaps": true,
                "target": "iPhone 7 Plus",
                "outDir": "${workspaceRoot}/.vscode/.react"
            }
        ]
    }